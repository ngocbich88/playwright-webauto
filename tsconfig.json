{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "node",
      "types": ["node"],
      "baseUrl": ".",
      "paths": {
        "@pages/*": ["pages/*"],
        "@support/*": ["features/support/*"]
      }
    },
    "include": ["features/**/*.ts", "pages/**/*.ts", "playwright.config.ts", "config.ts"]
}
  